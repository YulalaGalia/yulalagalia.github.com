{
  "manifest_version": 3,
  "name": "Найти дубли номеров",
  "version": "1.0",
  "description": "Расширение для поиска дублирующихся номеров на странице",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
